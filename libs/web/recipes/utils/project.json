{
  "projectType": "library",
  "root": "libs/web/recipes/utils",
  "sourceRoot": "libs/web/recipes/utils/src",
  "prefix": "recipes",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/libs/web/recipes/utils"
      ],
      "options": {
        "jestConfig": "libs/web/recipes/utils/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/web/recipes/utils/src/**/*.ts",
          "libs/web/recipes/utils/src/**/*.html"
        ]
      }
    }
  },
  "tags": [
    "platform:web",
    "type:util",
    "scope:recipes"
  ]
}
